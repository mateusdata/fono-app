{
  "expo": {
    "name": "Fonotherapp",
    "slug": "Fonotherapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "fonotherapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#159797",
      "androidStatusBar": {
        "backgroundColor": "white",
        "style": "light"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.projetofono.fonotherapp",
      "infoPlist": {
        "NSFileProtection": "CompleteUnlessOpen",
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification",
          "video"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.projetofono.Fonotherapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "7f207d32-dff2-4e9d-b31f-7a357711db25"
      }
    }
  }
}
